{
    "name": "pureware/pureware-cli",
    "description": "The missing command line interface for shopware 6",
    "type": "project",
    "license": "MIT",
    "bin": ["bin/pure"],
    "autoload": {
        "psr-4": {
            "Pure\\PurewareCli\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "repositories": [
        {
            "type": "path",
            "url": "~/dev/private/pure/template-generator"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7",
        "symfony/console": "^5.0|^6.0",
        "symfony/process": "^5.0|^6.0",
        "symfony/string": "^6.0",
        "ext-json": "*",
        "shopware/core": "^6.4",
        "pureware/template-generator": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3",
        "symfony/var-dumper": "^6.0"
    }
}
